{
  "name": "todo-app",
  "version": "1.0.0",
  "description": "Angular 2 Todo App",
  "scripts": {
    "db": "json-server --watch --port 4000 db.json",
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "concurrently --kill-others \"npm run db\" \"npm run server\""
  },
  "contributors": [
    "Rob Wormald <robwormald@gmail.com>",
    "PatrickJS <github@gdi2290.com>",
    "John Lindquist"
  ],
  "license": "MIT",
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^1.1.1",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "raw-loader": "^0.5.1",
    "to-string-loader": "^1.1.4",
    "typescript": "^2.0.2",
    "typings": "~1.0.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/platform-server": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "@ngrx/core": "^1.1.0",
    "@ngrx/effects": "^2.0.0-beta.3",
    "@ngrx/store": "^2.1.2",
    "core-js": "^2.4.0",
    "ie-shim": "^0.1.0",
    "json-server": "^0.8.19",
    "rxjs": "5.0.0-beta.11",
    "zone.js": "~0.6.17"
  },
  "keywords": [
    "Angular2",
    "Todo App"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnlindquist/todo-app.git"
  },
  "bugs": {
    "url": "https://github.com/johnlindquist/todo-app/issues"
  },
  "homepage": "https://github.com/johnlindquist/todo-app#readme"
}
